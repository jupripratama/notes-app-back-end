{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style":"off",
        "eol-last":"off",
        "indent":"off",
        "no-unused-vars":"off",
        "no-undef": "off",
        "no-trailing-spaces":"off",
        "no-multiple-empty-lines":"off",
        "object-curly-newline":"off"
    }
}
